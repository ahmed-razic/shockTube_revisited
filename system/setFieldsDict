FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    U       (0 0 0);
    T       0.0;
}

zones
{
    startingTemp
    {
        type        box;

        box         (-0.5 -1 -1) (0.5 1 1);

        values
        {
            T       1.0;
        }
    }
}

